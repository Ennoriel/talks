{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [
		{ "files": "*.svelte", "options": { "parser": "svelte" } },
		{ "files": "*.md", "options": { "tabWidth": 2 } }
	],
	"tabWidth": 4,
	"svelteBracketNewLine": true
}
